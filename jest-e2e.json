{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "roots": ["<rootDir>/__test__"],
  "testEnvironment": "node",
  "testRegex": ".*\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.(t|j)s"
  ],
  "coverageDirectory": "<rootDir>/coverage",
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  },
  "setupFilesAfterEnv": [],
  "testTimeout": 30000,
  "verbose": true,
  "detectOpenHandles": true,
  "forceExit": true
}
